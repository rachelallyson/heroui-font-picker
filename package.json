{
  "name": "@rachelallyson/heroui-font-picker",
  "version": "1.0.0",
  "type": "module",
  "description": "A powerful font picker component for HeroUI with Google Fonts integration and sprite-based previews",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "typedoc",
    "docs:api": "typedoc",
    "docs:dev": "cd docs && next dev",
    "docs:build": "cd docs && next build",
    "docs:api:watch": "typedoc --watch"
  },
  "keywords": [
    "heroui",
    "font-picker",
    "google-fonts",
    "react",
    "typescript",
    "ui-components",
    "font-preview",
    "sprite-system"
  ],
  "author": "Rachel Higley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rachelallyson/heroui-font-picker.git"
  },
  "bugs": {
    "url": "https://github.com/rachelallyson/heroui-font-picker/issues"
  },
  "homepage": "https://github.com/rachelallyson/heroui-font-picker#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "@heroui/autocomplete": {
      "optional": true
    },
    "@heroui/react": {
      "optional": true
    }
  },
  "dependencies": {
    "@react-aria/i18n": "^3.12.13"
  },
  "devDependencies": {
    "@heroui/autocomplete": "^2.3.29",
    "@heroui/react": "^2.8.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "next": "^16.0.1",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3"
  }
}